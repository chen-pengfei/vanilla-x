{
    "name": "vanduke/vanilla-x",
    "description": "",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "vanduke/vanilla-framework": "*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
